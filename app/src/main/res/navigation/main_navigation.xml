<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/mainFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.testking.ui.main.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/main_fragment" >
        <action
            android:id="@+id/action_mainFragment_to_photoDetail"
            app:destination="@id/photoDetail" />
    </fragment>
    <fragment
        android:id="@+id/photoDetail"
        android:name="com.example.testking.ui.main.PhotoDetailFragment"
        android:label="fragment_photo_detail"
        tools:layout="@layout/fragment_photo_detail" />
</navigation>